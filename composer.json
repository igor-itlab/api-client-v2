{
  "name": "itlab-studio/api-client-v2",
  "type": "library",
  "description": "Library for api call with different resources",
  "keywords": ["api", "resource"],
  "homepage": "https://itlab-studio.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "ITLab-Studio Development",
      "email": "igor-tolstoy@itlab-studio.com"
    }
  ],
  "repositories": [
    {
      "type":"package",
      "package": {
        "name": "itlab-studio/api-client-v2",
        "version":"master",
        "source": {
          "url": "https://github.com/igor-itlab/api-client-v2.git",
          "type": "git",
          "reference":"master"
        }
      }
    }
