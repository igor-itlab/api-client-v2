{
  "name": "itlab-studio/api-client-v2",
  "type": "library",
  "description": "Library for api call with different resources",
  "keywords": [
    "api",
    "resource"
  ],
  "homepage": "https://itlab-studio.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "ITLab-Studio Development",
      "email": "igor-tolstoy@itlab-studio.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "itlab-studio/api-client-v2",
        "version": "master",
        "source": {
          "url": "https://github.com/igor-itlab/api-client-v2.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "symfony/http-kernel": "*",
    "symfony/dependency-injection": "*",
    "symfony/config": "*",
    "symfony/http-client": "^5.3",
    "doctrine/annotations": "^1.13",
    "symfony/serializer": "^5.3",
    "symfony/property-access": "^5.3",
    "doctrine/collections": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "ApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApiClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "require": "^3.4 || ^4.0 || ^5.0"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "symfony/phpunit-bridge": "^5.3",
    "phpunit/phpunit": "^9.5",
    "symfony/test-pack": "^1.0"
  }
}
